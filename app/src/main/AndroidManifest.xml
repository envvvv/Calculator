<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".help"
            android:label="@string/title_activity_help"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".volume"
            android:label="@string/title_activity_volume"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".area"
            android:label="@string/title_activity_area"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".math"
            android:label="@string/title_activity_math"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".length"
            android:label="@string/title_activity_length"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".angle"
            android:label="@string/title_activity_angle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".turn"
            android:label="@string/title_activity_turn"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>